<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.StatisticsFormationController">
   <center>
      <AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #3D4956;">
         <children>
            <BarChart fx:id="barChart" layoutX="50.0">
              <xAxis>
                <CategoryAxis side="BOTTOM" fx:id="xAxis" />
              </xAxis>
              <yAxis>
                <NumberAxis side="LEFT" tickLabelFill="#ffd400" />
              </yAxis>
            </BarChart>
         </children>
      </AnchorPane>
   </center>
   <top>
      <AnchorPane prefHeight="74.0" prefWidth="600.0" style="-fx-background-color: #3D4956; -fx-border-color: #ffd400;" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#goback" prefHeight="27.0" prefWidth="59.0" style="-fx-border-color: #ffd400; -fx-background-color: #ffd400;" text="Back">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Text fill="#ffd400" layoutX="146.0" layoutY="49.0" strokeType="OUTSIDE" strokeWidth="0.0" text="statistique Formation">
               <font>
                  <Font name="System Bold" size="31.0" />
               </font>
            </Text>
         </children>
      </AnchorPane>
   </top>
</BorderPane>
